{
  "name": "Save with Info",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "description": "Save image with information",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["https://www.tumblr.com/*"],
    "js": [
      "content.js"
    ]
  }],
  "permissions": [
    "activeTab",
    "scripting",
    "contextMenus"
  ]
}
